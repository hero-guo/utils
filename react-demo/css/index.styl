body
  font-size 14px
  width 100%
  height 100%
:global
  #creator
    width 100%
    height 100%
    background #eff1f5
  .pull-left
    float left
  .pull-right
    float right
  .clearfix
    zoom: 1;
    &:before,
    &:after
      content: " ";
      display: table;
    &:after
      clear: both;
      visibility: hidden;
      font-size: 0;
      height: 0;
.content {
  margin: 0 auto;
  max-width: 1179px
}
